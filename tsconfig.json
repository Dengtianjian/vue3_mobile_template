{
  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNEXT",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": true,
    "moduleResolution": "Node",
    "charset": "utf8",
    "allowJs": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "inlineSourceMap": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "paths": {
      "@Api": [
        "./src/api/modules"
      ],
      "@CommonComponents": [
        "./src/components/common"
      ],
      "@FrontComponents": [
        "./src/components/front"
      ],
      "@FrontViews": [
        "./src/views/front"
      ],
      "@Service": [
        "./src/service"
      ],
      "@SystemViews": [
        "./src/views/system"
      ]
    },
    "types": []
  },
  "exclude": [
    "app/public",
    "app/views",
    "node_modules*",
    "dist",
    "public",
    "outputs",
    "*.js"
  ]
}